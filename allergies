public class Allergies {
  private String name_all;
  private int code_all;
  private string description_all;
  
  public String getdescription_all(){
   return description_all;
   }
   public void setdescription_all(String description_all){
   this.description_all=description_all;
   }
    public String getname_all() {
    return name_all;
  }
  public void setname_all(String name_all) {
    this.name_all = name_all;
	}
	public int getcode_all() {
    return code_all;
  }
  public void setcode_all(int code_all) {
    this.code_all = code_all;
	}
	}